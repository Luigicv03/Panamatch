{
  "cli": {
    "version": ">= 5.9.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://panamatch-backend.onrender.com",
        "EXPO_PUBLIC_SOCKET_URL": "https://panamatch-backend.onrender.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}

